{
  "expo": {
    "name": "Todoist Clone",
    "slug": "todoist-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "scheme": "todoist-clone",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1f1f1f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.todoist.clone",
      "deploymentTarget": "18.0"
    },
    "android": {
      "package": "com.todoist.clone"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3ac0a83c-64f6-4fd5-ae18-ddf8c8ce7239"
      }
    },
    "owner": "black-canaries"
  }
}
